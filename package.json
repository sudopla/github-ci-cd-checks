{
  "name": "cdk_infra",
  "version": "0.1.0",
  "bin": {
    "cdk_infra": "bin/cdk_infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint-cdk": "eslint -c ./.eslintrc.yml --no-eslintrc --ext=.ts cdk_infra/",
    "lint-cdk-fix": "eslint -c ./.eslintrc.yml --no-eslintrc --ext=.ts --fix cdk_infra/",
    "upgrade:cdk": "yarn upgrade --latest && yarn"
  },
  "dependencies": {
    "@aws-cdk/aws-events": "^1.115.0",
    "@aws-cdk/aws-events-targets": "^1.115.0",
    "@aws-cdk/aws-iam": "^1.115.0",
    "@aws-cdk/aws-lambda": "^1.115.0",
    "@aws-cdk/aws-lambda-python": "^1.115.0",
    "@aws-cdk/core": "1.115.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.115.0",
    "@types/jest": "^26.0.24",
    "@types/node": "16.4.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "aws-cdk": "1.115.0",
    "eslint": "^7.31.0",
    "eslint-plugin-cdk": "^1.0.2",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "~4.3.5",
    "yarn": "^1.22.10"
  }
}
